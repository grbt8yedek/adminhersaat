{
  "name": "grbt8ap-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3004",
    "build": "node vercel-protection.js && prisma generate && prisma db push --accept-data-loss && next build",
    "start": "next start -p 3004",
    "lint": "next lint",
    "deploy": "bash deploy.sh",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "prisma:push": "node scripts/prisma-guard.js && prisma db push",
    "prisma:push-force": "node scripts/prisma-guard-advanced.js && prisma db push --force-reset",
    "prisma:migrate": "node scripts/prisma-guard.js && prisma migrate dev",
    "prisma:migrate-reset": "node scripts/prisma-guard-advanced.js && prisma migrate reset",
    "prisma:seed": "node scripts/prisma-guard.js && prisma db seed",
    "backup:auto": "node scripts/auto-backup.js",
    "backup:list": "node scripts/auto-backup.js list",
    "backup:prisma": "node scripts/auto-backup.js prisma",
    "guard:check": "node scripts/prisma-guard.js --check"
  },
  "dependencies": {
    "@prisma/client": "5.10.2",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-tabs": "1.1.12",
    "@tanstack/react-query": "5.83.0",
    "@types/pdfkit": "0.17.2",
    "@types/pg": "8.15.5",
    "bcrypt": "6.0.0",
    "date-fns": "4.1.0",
    "isomorphic-dompurify": "^2.8.0",
    "lucide-react": "0.525.0",
    "next": "13.5.6",
    "next-auth": "4.24.11",
    "otplib": "^12.0.1",
    "pdfkit": "0.17.2",
    "pg": "8.16.3",
    "prisma": "5.10.2",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "7.60.0",
    "recharts": "3.1.0",
    "resend": "6.1.0",
    "zod": "4.0.5"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
